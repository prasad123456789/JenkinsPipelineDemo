    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
             stage('Build') {
            steps {
                echo 'Build Successfully'
            }
            
        }
        stage('Deploy') {
            steps {
                echo 'Deploy Successfully'
            }
            
        }
        stage('Test') {
            steps {
                echo 'Test Successfully'
            }
            
        }
        stage('Release') {
            steps {
                echo 'Release Successfully'
            }
            
        }
    }
}

